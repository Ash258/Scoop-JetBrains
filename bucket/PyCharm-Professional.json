{
    "version": "2019.1.1-191.6605.12",
    "description": "PyCharm Professional. Cross-Platform Python IDE for professional developers by JetBrains.",
    "homepage": "https://www.jetbrains.com/pycharm/",
    "url": "https://download.jetbrains.com/python/pycharm-professional-2019.1.1.exe#/cosi.7z",
    "hash": "08b9c5563c35200e4661ea8eae9ae4043b0f69986640a2e608d9a76ff96361aa",
    "post_install": "Remove-Item \"$dir\\`$*\" -Force -Recurse",
    "persist": "bin\\idea.properties",
    "architecture": {
        "64bit": {
            "bin": [
                "bin\\pycharm.bat",
                "bin\\pycharm64.exe"
            ],
            "shortcuts": [
                [
                    "bin\\pycharm64.exe",
                    "PyCharm Professional"
                ]
            ]
        },
        "32bit": {
            "bin": "bin\\pycharm.exe",
            "shortcuts": [
                [
                    "bin\\pycharm.exe",
                    "PyCharm Professional"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=PCP&latest=true&platform=zip&type=release",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/python/pycharm-professional-$matchHead.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
