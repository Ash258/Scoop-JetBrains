{
    "version": "2019.2-192.5728.105",
    "description": "PyCharm Professional. Cross-Platform Python IDE for professional developers by JetBrains.",
    "homepage": "https://www.jetbrains.com/pycharm/",
    "url": "https://download.jetbrains.com/python/pycharm-professional-2019.2.exe#/cosi.7z",
    "hash": "aa75e24563566e95b88a495ae931424985df52761913c2e8525276b2465cc8e5",
    "extract_to": "IDE",
    "post_install": "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse",
    "persist": "IDE\\bin\\idea.properties",
    "architecture": {
        "64bit": {
            "bin": [
                "IDE\\bin\\pycharm.bat",
                "IDE\\bin\\pycharm64.exe"
            ],
            "shortcuts": [
                [
                    "IDE\\bin\\pycharm64.exe",
                    "PyCharm Professional"
                ]
            ]
        },
        "32bit": {
            "bin": "IDE\\bin\\pycharm.exe",
            "shortcuts": [
                [
                    "IDE\\bin\\pycharm.exe",
                    "PyCharm Professional"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=PCP&latest=true&platform=zip&type=release",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/python/pycharm-professional-$matchHead.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
