{
    "version": "2019.3-193.3793.20",
    "description": "GoLand Early Access Program. Cross-Platform, Clever IDE to Go by JetBrains.",
    "homepage": "https://www.jetbrains.com/goland/",
    "url": "https://download.jetbrains.com/go/goland-193.3793.20.exe#/cosi.7z",
    "hash": "7b1d47b37b61730ee39cd5cdeeea5ac3ea08836aab51364116ee0bd4e473c000",
    "extract_to": "IDE",
    "post_install": "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse",
    "persist": "IDE\\bin\\idea.properties",
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\goland64.exe",
                    "GoLand (Early Access Program)"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\goland.exe",
                    "GoLand (Early Access Program)"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=GO&latest=true&platform=zip&type=eap",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/go/goland-$preReleaseVersion.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
