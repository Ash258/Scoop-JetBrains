{
    "version": "2019.2-192.5728.12",
    "description": "IntelliJ IDEA Community Early Access Program. Cross-Platform Java IDE for professional developers by JetBrains.",
    "homepage": "https://www.jetbrains.com/idea/",
    "url": "https://download.jetbrains.com/idea/ideaIC-192.5728.12.win.zip",
    "hash": "81bc6005449228e5e378c75b09bbf56f965da43068a312e6557ee9760b0cabf4",
    "extract_to": "IDE",
    "post_install": "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse",
