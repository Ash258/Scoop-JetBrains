{
    "version": "2019.2.2-192.6603.23",
    "description": "IntelliJ IDEA Community Release Candidate. Cross-Platform Java IDE for professional developers by JetBrains.",
    "homepage": "https://www.jetbrains.com/idea/",
    "url": "https://download.jetbrains.com/idea/ideaIC-192.6603.23.win.zip",
    "hash": "d95a26b0fdee9b6a8d9091b732ee29b275026901bebb5f05c0a6dd6b201f3a40",
    "extract_to": "IDE",
    "post_install": "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse",
    "persist": "IDE\\bin\\idea.properties",
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea64.exe",
                    "IntelliJ IDEA Community (Release Candidate)"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea.exe",
                    "IntelliJ IDEA Community (Release Candidate)"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=IIC&latest=true&platform=zip&type=rc",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/idea/ideaIC-$preReleaseVersion.win.zip",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
