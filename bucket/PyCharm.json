{
    "version": "2019.1.1-191.6605.12",
    "description": "PyCharm Community. Cross-Platform Python IDE for professional developers by JetBrains.",
    "homepage": "https://www.jetbrains.com/pycharm/",
    "url": "https://download.jetbrains.com/python/pycharm-community-2019.1.1.exe#/cosi.7z",
    "hash": "505af5a257d87402f2dc566cac618b3fcbe131c0499072c7963b56549c77729b",
    "post_install": "Remove-Item \"$dir\\`$*\" -Force -Recurse",
    "persist": "bin\\idea.properties",
    "architecture": {
        "64bit": {
            "bin": [
                "bin\\pycharm.bat",
                "bin\\pycharm64.exe"
            ],
            "shortcuts": [
                [
                    "bin\\pycharm64.exe",
                    "PyCharm Community"
                ]
            ]
        },
        "32bit": {
            "bin": "bin\\pycharm.exe",
            "shortcuts": [
                [
                    "bin\\pycharm.exe",
                    "PyCharm Community"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=PCC&latest=true&platform=zip&type=release",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/python/pycharm-community-$matchHead.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
