{
    "version": "2019.2.4-192.7142.26",
    "description": "IntelliJ IDEA Community Release Candidate. Cross-Platform Java IDE for professional developers by JetBrains.",
    "homepage": "https://www.jetbrains.com/idea/",
    "license": {
        "identifier": "Proprietary",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "url": [
        "https://download.jetbrains.com/idea/ideaIC-192.7142.26.win.zip",
        "https://raw.githubusercontent.com/Ash258/Scoop-JetBrains/master/scripts/portable.ps1"
    ],
    "hash": [
        "382a69367de8f8b6be842b1bc87852dbf5a92e94f1611ffc958e85cc87927eff",
        "196d75d69940f3d2f203c02e1105cf80ab1a15558844a30fae2e84284ff26d0c"
    ],
    "extract_to": "IDE",
    "installer": {
        "script": [
            "& \"$dir\\portable.ps1\" $dir $persist_dir",
            "Remove-Item \"$dir\\IDE\\`$*\", \"$dir\\portable.ps1\" -Force -Recurse"
        ]
    },
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea64.exe",
                    "JetBrains\\IntelliJ IDEA Community (Release Candidate)"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea.exe",
                    "JetBrains\\IntelliJ IDEA Community (Release Candidate)"
                ]
            ]
        }
    },
    "persist": [
        "IDE\\bin\\idea.properties",
        "profile"
    ],
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=IIC&latest=true&platform=zip&type=rc",
        "regex": "version\":\"(?<ver>[\\d.]+)\".*\"build\":\"(?<build>[\\d.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/idea/ideaIC-$preReleaseVersion.win.zip",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
