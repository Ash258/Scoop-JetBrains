{
    "version": "2019.2.4-192.7142.30",
    "description": "PyCharm Professional Release Candidate. Cross-Platform Python IDE for professional developers by JetBrains.",
    "homepage": "https://www.jetbrains.com/pycharm/",
    "url": "https://download.jetbrains.com/python/pycharm-professional-192.7142.30.exe#/cosi.7z",
    "hash": "3f467cc8c74e58e3cac5c0038fdb4df08cee46134814870b827c14f044f563f6",
    "extract_to": "IDE",
    "post_install": "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse",
    "persist": "IDE\\bin\\idea.properties",
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\pycharm64.exe",
                    "PyCharm Professional (Release Candidate)"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\pycharm.exe",
                    "PyCharm Professional (Release Candidate)"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=PCP&latest=true&platform=zip&type=rc",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/python/pycharm-professional-$preReleaseVersion.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
