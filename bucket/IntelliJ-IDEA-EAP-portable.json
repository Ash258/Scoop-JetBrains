    "homepage": "https://www.jetbrains.com/idea/",
    "license": {
        "identifier": "Apache-2.0",
        "url": "https://sales.jetbrains.com/hc/en-gb/articles/115001015290-Where-can-I-find-the-EULA-End-User-License-Agreement-"
    },
    "url": [
        "https://download.jetbrains.com/idea/ideaIC-193.5233.12.win.zip",
        "https://raw.githubusercontent.com/Ash258/Scoop-JetBrains/master/scripts/portable.ps1"
    ],
    "hash": [
        "9aa8b7b38cd2c4d1bd519918529c63d0f0b6dee77f801500ad48244290844bc0",
        "196d75d69940f3d2f203c02e1105cf80ab1a15558844a30fae2e84284ff26d0c"
    ],
    "extract_to": "IDE",
    "installer": {
        "script": [
            "& \"$dir\\portable.ps1\" $dir $persist_dir",
            "Remove-Item \"$dir\\IDE\\`$*\", \"$dir\\portable.ps1\" -Force -Recurse"
        ]
    },
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea64.exe",
                    "JetBrains\\IntelliJ IDEA Community (Early Access Program)"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea.exe",
                    "JetBrains\\IntelliJ IDEA Community (Early Access Program)"
                ]
            ]
        }
    },
    "persist": [
        "IDE\\bin\\idea.properties",
        "profile"
    ],
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=IIC&latest=true&platform=zip&type=eap",
        "regex": "version\":\"(?<ver>[\\d.]+)\".*\"build\":\"(?<build>[\\d.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/idea/ideaIC-$preReleaseVersion.win.zip",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
