{
    "version": "2019.2-192.4787.12",
    "description": "CLion Early Access Program. Cross-Platform IDE for C and C++ by JetBrains.",
    "homepage": "https://www.jetbrains.com/cpp/",
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/cpp/CLion-192.4787.12.win.zip",
            "hash": "5b50956cb67c4dccc8490b64c6b55ff6b1f7eb51c27a2a7000eee060e1a4efa5",
            "shortcuts": [
                [
                    "IDE\\bin\\clion64.exe",
                    "CLion (Early Access Program)"
                ]
            ]
        }
    },
    "extract_to": "IDE",
    "post_install": "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse",
    "persist": "IDE\\bin\\idea.properties",
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=CL&latest=true&platform=zip&type=eap",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.jetbrains.com/cpp/CLion-$preReleaseVersion.win.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
