{
    "version": "2019.3-193.4099.13",
    "description": "IntelliJ IDEA Ultimate Early Access Program. Cross-Platform Java IDE for professional developers by JetBrains.",
    "homepage": "https://www.jetbrains.com/idea/",
    "url": "https://download.jetbrains.com/idea/ideaIU-193.4099.13.win.zip",
    "hash": "1bc9a5ee02f4bb9de4be4a6c9f7a5dbe0855728ef22784e56de1588a41fce1bb",
    "extract_to": "IDE",
    "post_install": "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse",
    "persist": "IDE\\bin\\idea.properties",
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea64.exe",
                    "IntelliJ IDEA Ultimate (Early Access Program)"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\idea.exe",
                    "IntelliJ IDEA Ultimate (Early Access Program)"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=IIU&latest=true&platform=zip&type=eap",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/idea/ideaIU-$preReleaseVersion.win.zip",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
