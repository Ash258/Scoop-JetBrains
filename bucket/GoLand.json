{
    "version": "2019.2.1-192.6262.60",
    "description": "GoLand. Cross-Platform, Clever IDE to Go by JetBrains.",
    "homepage": "https://www.jetbrains.com/goland/",
    "url": "https://download.jetbrains.com/go/goland-2019.2.1.exe#/cosi.7z",
    "hash": "6a7f2dfcd7de3ac80e6a4de33638de2c6cc8e180cbc934a6c08d125d9599e128",
    "extract_to": "IDE",
    "post_install": "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse",
    "persist": "IDE\\bin\\idea.properties",
    "architecture": {
        "64bit": {
            "bin": [
                "IDE\\bin\\goland.bat",
                "IDE\\bin\\goland64.exe"
            ],
            "shortcuts": [
                [
                    "IDE\\bin\\goland64.exe",
                    "GoLand"
                ]
            ]
        },
        "32bit": {
            "bin": "IDE\\bin\\goland.exe",
            "shortcuts": [
                [
                    "IDE\\bin\\goland.exe",
                    "GoLand"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=GO&latest=true&platform=zip&type=release",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/go/goland-$matchHead.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
