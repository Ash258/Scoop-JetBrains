{
    "version": "2019.2.3-192.6817.16",
    "description": "RubyMine. Cross-Platform Ruby on Rails IDE by JetBrains.",
    "homepage": "https://www.jetbrains.com/rubymine/",
    "url": "https://download.jetbrains.com/ruby/RubyMine-2019.2.3.exe#/cosi.7z",
    "hash": "21946ebb67d45024283c28924d951e451ec62ef1d75ef966beab484c71c6101f",
    "extract_to": "IDE",
    "post_install": "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse",
    "persist": "IDE\\bin\\idea.properties",
    "architecture": {
        "64bit": {
            "bin": [
                "IDE\\bin\\rubymine.bat",
                "IDE\\bin\\rubymine64.exe"
            ],
            "shortcuts": [
                [
                    "IDE\\bin\\rubymine64.exe",
                    "RubyMine"
                ]
            ]
        },
        "32bit": {
            "bin": "IDE\\bin\\rubymine.exe",
            "shortcuts": [
                [
                    "IDE\\bin\\rubymine.exe",
                    "RubyMine"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=RM&latest=true&platform=zip&type=release",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/ruby/RubyMine-$matchHead.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
