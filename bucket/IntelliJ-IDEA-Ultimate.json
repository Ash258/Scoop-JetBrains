{
    "version": "2019.2.4-192.7142.36",
    "description": "IntelliJ IDEA Ultimate. Cross-Platform Java IDE for professional developers by JetBrains.",
    "homepage": "https://www.jetbrains.com/idea/",
    "url": "https://download.jetbrains.com/idea/ideaIU-2019.2.4.win.zip",
    "hash": "bbdf6957e3b8f230f24fdf28c724c14633677aa2bc8915c2d7e906ef23da67a0",
    "extract_to": "IDE",
    "post_install": "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse",
    "persist": "IDE\\bin\\idea.properties",
    "architecture": {
        "64bit": {
            "bin": [
                "IDE\\bin\\idea.bat",
                "IDE\\bin\\idea64.exe"
            ],
            "shortcuts": [
                [
                    "IDE\\bin\\idea64.exe",
                    "IntelliJ IDEA Ultimate"
                ]
            ]
        },
        "32bit": {
            "bin": "IDE\\bin\\idea.exe",
            "shortcuts": [
                [
                    "IDE\\bin\\idea.exe",
                    "IntelliJ IDEA Ultimate"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=IIU&latest=true&platform=zip&type=release",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/idea/ideaIU-$matchHead.win.zip",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
