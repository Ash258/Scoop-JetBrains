{
    "version": "2019.3-193.5233.15",
    "description": "PyCharm Professional Early Access Program. Cross-Platform Python IDE for professional developers by JetBrains.",
    "homepage": "https://www.jetbrains.com/pycharm/",
    "license": {
        "identifier": "Proprietary",
        "url": "https://www.jetbrains.com/legal/agreements/user_eap.html"
    },
    "url": "https://download.jetbrains.com/python/pycharm-professional-193.5233.15.exe#/cosi.7z",
    "hash": "3130a6ce3c861718d1c912c2e6255a35778f1650c8c1c067cbb03cbbff8a13bb",
    "extract_to": "IDE",
    "installer": {
        "script": [
            "$split = Split-Path $dir",
            "$currentDir = \"$split\\current\" -replace '\\\\', '/'",
            "$IDE = \"$dir\\IDE\"",
            "$file = 'IDE\\bin\\idea.properties'",
            "if (-not (Test-Path \"$persist_dir\\$file\")) {",
            "    Write-Host 'File' $file 'does not exists. Creating.' -f Yellow",
            "    $file = \"$dir\\$file\"",
            "    $profileDir = \"$currentDir/profile\"",
            "    $CONT = Get-Content $file",
            "        # Replacing config options",
            "    $CONT = $CONT -replace '^#\\s*idea.config.path=.*$', \"idea.config.path=$profileDir/config\"",
            "    $CONT = $CONT -replace '^#\\s*idea.system.path=.*$', \"idea.system.path=$profileDir/system\"",
            "    $CONT = $CONT -replace '^#\\s*idea.plugins.path=', 'idea.plugins.path='",
            "    $CONT = $CONT -replace '^#\\s*idea.log.path=', 'idea.log.path='",
            "        # Rewrite properties file",
            "    Set-Content $file $CONT -Encoding Ascii -Force",
            "}",
            "Remove-Item \"$IDE\\`$*\" -Force -Recurse"
        ]
    },
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\pycharm64.exe",
                    "JetBrains\\PyCharm Professional (Early Access Program)"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\pycharm.exe",
                    "JetBrains\\PyCharm Professional (Early Access Program)"
                ]
            ]
        }
    },
    "persist": [
        "IDE\\bin\\idea.properties",
        "profile"
    ],
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=PCP&latest=true&platform=zip&type=eap",
        "regex": "version\":\"(?<ver>[\\d.]+)\".*\"build\":\"(?<build>[\\d.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/python/pycharm-professional-$preReleaseVersion.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
