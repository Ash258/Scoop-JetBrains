{
    "version": "2019.3-193.4386.20",
    "description": "PhpStorm Early Access Program. Cross-Platform, Lightning-Smart IDE for PHP programming by JetBrains.",
    "homepage": "https://www.jetbrains.com/phpstorm/",
    "url": "https://download.jetbrains.com/webide/PhpStorm-193.4386.20.exe#/cosi.7z",
    "hash": "b4794790a4711664ce0941319f1071c1d85f2a7e1921dd9e83b49e6321cfd75a",
    "extract_to": "IDE",
    "post_install": "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse",
    "persist": "IDE\\bin\\idea.properties",
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\phpstorm64.exe",
                    "PhpStorm (Early Access Program)"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\phpstorm.exe",
                    "PhpStorm (Early Access Program)"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=PS&latest=true&platform=zip&type=eap",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/webide/PhpStorm-$preReleaseVersion.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
