{
    "version": "2018.3.4-183.5429.34",
    "description": "WebStorm. Cross-Platform, Smart JavaScript IDE by JetBrains.",
    "homepage": "https://www.jetbrains.com/webstorm/",
    "url": "https://download.jetbrains.com/webstorm/WebStorm-2018.3.4.exe#/cosi.7z",
    "hash": "4f794eb0cf3e77f67f4c09ec6b71fb71662666fd71afe04f7084725e5f5d3448",
    "post_install": "Remove-Item \"$dir\\`$*\" -Force -Recurse",
    "persist": "bin\\idea.properties",
    "architecture": {
        "64bit": {
            "bin": [
                "bin\\webstorm.bat",
                "bin\\webstorm64.exe"
            ],
            "shortcuts": [
                [
                    "bin\\webstorm64.exe",
                    "WebStorm"
                ]
            ]
        },
        "32bit": {
            "bin": "bin\\webstorm.exe",
            "shortcuts": [
                [
                    "bin\\webstorm.exe",
                    "WebStorm"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=WS&latest=true&platform=zip&type=release",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/webstorm/WebStorm-$matchHead.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
