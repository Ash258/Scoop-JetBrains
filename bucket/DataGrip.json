{
    "version": "2018.3.3-183.5912.12",
    "description": "DataGrip. Cross-Platform IDE for Databases & SQL by JetBrains.",
    "homepage": "https://www.jetbrains.com/datagrip/",
    "url": "https://download.jetbrains.com/datagrip/datagrip-2018.3.3.exe#/cosi.7z",
    "hash": "792ac36cc28ed8f711252ecd2c2b6339ed7a8a154ee89ad0f4b689d43ea25bdd",
    "post_install": "Remove-Item \"$dir\\`$*\" -Force -Recurse",
    "persist": "bin\\idea.properties",
    "architecture": {
        "64bit": {
            "bin": [
                "bin\\datagrip.bat",
                "bin\\datagrip64.exe"
            ],
            "shortcuts": [
                [
                    "bin\\datagrip64.exe",
                    "DataGrip"
                ]
            ]
        },
        "32bit": {
            "bin": "bin\\datagrip.exe",
            "shortcuts": [
                [
                    "bin\\datagrip.exe",
                    "DataGrip"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=DG&latest=true&platform=zip&type=release",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/datagrip/datagrip-$matchHead.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
