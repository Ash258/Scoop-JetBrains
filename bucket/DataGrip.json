{
    "version": "2018.3.2-183.5429.24",
    "description": "DataGrip. Cross-Platform IDE for Databases & SQL by JetBrains.",
    "homepage": "https://www.jetbrains.com/datagrip/",
    "url": "https://download.jetbrains.com/datagrip/datagrip-2018.3.2.exe#/cosi.7z",
    "hash": "cd6a7712988c8d768e2c79d5c9269f767e5b48d7e134b22d85fb686040e7e40e",
    "post_install": "Remove-Item \"$dir\\`$*\" -Force -Recurse",
    "persist": "bin\\idea.properties",
    "architecture": {
        "64bit": {
            "bin": [
                "bin\\datagrip.bat",
                "bin\\datagrip64.exe"
            ],
            "shortcuts": [
                [
                    "bin\\datagrip64.exe",
                    "DataGrip"
                ]
            ]
        },
        "32bit": {
            "bin": "bin\\datagrip.exe",
            "shortcuts": [
                [
                    "bin\\datagrip.exe",
                    "DataGrip"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=DG&latest=true&platform=zip&type=release",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/datagrip/datagrip-$matchVer.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
