{
    "version": "2018.3.2-183.4886.39",
    "description": "CLion. Cross-Platform IDE for C and C++ by JetBrains.",
    "homepage": "https://www.jetbrains.com/cpp/",
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/cpp/CLion-2018.3.2.zip",
            "hash": "24169c45759a81486fcbcdd5e6a952843a2f6567cb84b8d1b8007c28a7705be5",
            "bin": [
                "bin\\clion.bat",
                "bin\\clion64.exe"
            ],
            "shortcuts": [
                [
                    "bin\\clion64.exe",
                    "CLion"
                ]
            ]
        }
    },
    "persist": "bin\\idea.properties",
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=CL&latest=true&platform=zip&type=release",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.jetbrains.com/cpp/CLion-$matchVer.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
