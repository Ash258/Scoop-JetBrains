{
    "version": "2019.2.3-192.6817.5",
    "description": "RubyMine Early Access Program. Cross-Platform Ruby on Rails IDE by JetBrains.",
    "homepage": "https://www.jetbrains.com/rubymine/",
    "url": "https://download.jetbrains.com/ruby/RubyMine-192.6817.5.exe#/cosi.7z",
    "hash": "f49e63b9a302094411e23db59dc6b5c67bb571d5d1fd8f789bb0ef22ff5281b1",
    "extract_to": "IDE",
    "post_install": "Remove-Item \"$dir\\IDE\\`$*\" -Force -Recurse",
    "persist": "IDE\\bin\\idea.properties",
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\rubymine64.exe",
                    "RubyMine (Early Access Program)"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "IDE\\bin\\rubymine.exe",
                    "RubyMine (Early Access Program)"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=RM&latest=true&platform=zip&type=eap",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/ruby/RubyMine-$preReleaseVersion.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
