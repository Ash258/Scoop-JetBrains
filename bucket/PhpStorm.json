{
    "version": "2018.3.4-183.5429.47",
    "description": "PhpStorm. Cross-Platform, Lightning-Smart IDE for PHP programming by JetBrains.",
    "homepage": "https://www.jetbrains.com/phpstorm/",
    "url": "https://download.jetbrains.com/webide/PhpStorm-2018.3.4.exe#/cosi.7z",
    "hash": "990802675a03f65ba792f2660398e06694d16919c7a1d3abcd839f5cf2b00397",
    "post_install": "Remove-Item \"$dir\\`$*\" -Force -Recurse",
    "persist": "bin\\idea.properties",
    "architecture": {
        "64bit": {
            "bin": [
                "bin\\phpstorm.bat",
                "bin\\phpstorm64.exe"
            ],
            "shortcuts": [
                [
                    "bin\\phpstorm64.exe",
                    "PhpStorm"
                ]
            ]
        },
        "32bit": {
            "bin": "bin\\phpstorm.exe",
            "shortcuts": [
                [
                    "bin\\phpstorm.exe",
                    "PhpStorm"
                ]
            ]
        }
    },
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=PS&latest=true&platform=zip&type=release",
        "regex": "version\":\"(?<ver>[\\d\\.]+)\".*\"build\":\"(?<build>[\\d\\.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/webide/PhpStorm-$matchHead.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
